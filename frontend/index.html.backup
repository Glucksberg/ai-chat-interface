    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Função para atualizar as informações de posição
            function updatePositionInfo() {
                // Posto de combustível
                const fuelStation = document.querySelector('.fuel-station');
                const fuelStyle = window.getComputedStyle(fuelStation);
                const fuelTop = parseInt(fuelStyle.top) || 0;
                const fuelLeft = parseInt(fuelStyle.left) || 0;
                document.getElementById('fuel-position').innerHTML = `Posição: top: ${fuelTop}px, left: ${fuelLeft}px`;
                
                // Armazém de grãos
                const grainStorage = document.querySelector('.grain-storage');
                const grainStyle = window.getComputedStyle(grainStorage);
                const grainTop = parseInt(grainStyle.top) || 0;
                const grainLeft = parseInt(grainStyle.left) || 0;
                document.getElementById('grain-position').innerHTML = `Posição: top: ${grainTop}px, left: ${grainLeft}px`;
            }
            
            // Atualizar as informações de posição quando a página carrega
            updatePositionInfo();
            
            // Atualizar as informações de posição quando os botões são clicados
            const buttons = document.querySelectorAll('button');
            buttons.forEach(button => {
                button.addEventListener('click', function() {
                    setTimeout(updatePositionInfo, 100);
                });
            });
            
            // Atalho de teclado para mover os ícones
            document.addEventListener('keydown', function(event) {
                const fuelStation = document.querySelector('.fuel-station');
                const grainStorage = document.querySelector('.grain-storage');
                const step = 10;
                
                // Mover o posto de combustível com as teclas WASD
                if (event.key.toLowerCase() === 'w') {
                    const top = parseInt(getComputedStyle(fuelStation).top) - step;
                    fuelStation.style.top = top + 'px';
                }
                if (event.key.toLowerCase() === 's') {
                    const top = parseInt(getComputedStyle(fuelStation).top) + step;
                    fuelStation.style.top = top + 'px';
                }
                if (event.key.toLowerCase() === 'a') {
                    const left = parseInt(getComputedStyle(fuelStation).left) - step;
                    fuelStation.style.left = left + 'px';
                }
                if (event.key.toLowerCase() === 'd') {
                    const left = parseInt(getComputedStyle(fuelStation).left) + step;
                    fuelStation.style.left = left + 'px';
                }
                
                // Mover o armazém de grãos com as teclas de seta
                if (event.key === 'ArrowUp') {
                    const top = parseInt(getComputedStyle(grainStorage).top) - step;
                    grainStorage.style.top = top + 'px';
                }
                if (event.key === 'ArrowDown') {
                    const top = parseInt(getComputedStyle(grainStorage).top) + step;
                    grainStorage.style.top = top + 'px';
                }
                if (event.key === 'ArrowLeft') {
                    const left = parseInt(getComputedStyle(grainStorage).left) - step;
                    grainStorage.style.left = left + 'px';
                }
                if (event.key === 'ArrowRight') {
                    const left = parseInt(getComputedStyle(grainStorage).left) + step;
                    grainStorage.style.left = left + 'px';
                }
                
                updatePositionInfo();
            });    
            
            // Variáveis para o chat
            const chatOverlay = document.getElementById('chat-overlay');
            const assistenteBtn = document.getElementById('assistente-btn');
            const closeChat = document.getElementById('close-chat');
            const chatMessages = document.getElementById('chatMessages');
            const userInput = document.getElementById('userInput');
            const sendButton = document.getElementById('sendButton');
            const typingIndicator = document.getElementById('typingIndicator');
            const suggestionChips = document.querySelectorAll('.suggestion-chip');
            
            // Histórico de mensagens para enviar ao servidor
            let messageHistory = [];
            
            // Abrir chat ao clicar no botão Assistente com efeito suave
            if (assistenteBtn) {
                assistenteBtn.addEventListener('click', () => {
                    chatOverlay.classList.add('active');
                    // Focar no input após abrir o chat
                    setTimeout(() => {
                        userInput.focus();
                    }, 400);
                });
            }
            
            // Fechar chat ao clicar no botão de fechar ou fora da janela de chat
            if (closeChat) {
                closeChat.addEventListener('click', () => {
                    chatOverlay.classList.remove('active');
                });
            }
            
            // Fechar chat ao clicar fora da janela
            chatOverlay.addEventListener('click', (e) => {
                if (e.target === chatOverlay) {
                    chatOverlay.classList.remove('active');
                }
            });
            
            // Função para adicionar mensagem ao chat
            function addMessage(content, isUser) {
                const messageDiv = document.createElement('div');
                messageDiv.classList.add('message');
                messageDiv.classList.add(isUser ? 'user-message' : 'ai-message');
                messageDiv.textContent = content;
                chatMessages.appendChild(messageDiv);
                chatMessages.scrollTop = chatMessages.scrollHeight;
                
                // Atualizar histórico de mensagens
                if (isUser) {
                    messageHistory.push({ role: 'user', content });
                } else {
                    messageHistory.push({ role: 'assistant', content });
                }
            }
            
            // Função para enviar mensagem para a API
            async function sendMessage(message) {
                try {
                    // Mostrar indicador de digitação
                    typingIndicator.style.display = 'block';
                    chatMessages.scrollTop = chatMessages.scrollHeight;
                    
                    sendButton.disabled = true;
                    userInput.disabled = true;
                    
                    console.log('Enviando mensagens:', messageHistory);
                    
                    // Verificar se há mensagens no histórico
                    if (messageHistory.length === 0) {
                        // Adicionar a primeira mensagem do usuário se o histórico estiver vazio
                        messageHistory.push({ role: 'user', content: message });
                    }
                    
                    console.log('Enviando histórico para API:', JSON.stringify(messageHistory));
                    
                    // Enviar o histórico completo de mensagens para contexto
                    const response = await fetch('/api/chat', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({
                            messages: messageHistory
                        })
                    });
                    
                    // Esconder indicador de digitação
                    typingIndicator.style.display = 'none';
                    
                    // Verificar se a resposta é válida
                    if (!response.ok) {
                        const errorText = await response.text();
                        console.error('Erro na resposta do servidor:', response.status, errorText);
                        addMessage(`Desculpe, ocorreu um erro no servidor (${response.status}). Por favor, tente novamente mais tarde.`, false);
                        return;
                    }
                    
                    const data = await response.json();
                    console.log('Resposta recebida:', data);
                    
                    if (data.success) {
                        addMessage(data.message, false);
                    } else {
                        const errorMsg = data.error || data.message || 'Erro desconhecido';
                        addMessage(`Desculpe, ocorreu um erro: ${errorMsg}`, false);
                        console.error('Detalhes do erro:', data);
                    }
                } catch (error) {
                    // Esconder indicador de digitação
                    typingIndicator.style.display = 'none';
                    
                    // Mostrar mensagem de erro
                    addMessage('Desculpe, ocorreu um erro ao conectar ao servidor. Por favor, verifique sua conexão e tente novamente.', false);
                    console.error('Erro:', error);
                } finally {
                    sendButton.disabled = false;
                    userInput.disabled = false;
                    userInput.focus();
                }
            }
            
            // Evento de clique no botão de enviar
            if (sendButton) {
                sendButton.addEventListener('click', () => {
                    const message = userInput.value.trim();
                    if (message) {
                        addMessage(message, true);
                        userInput.value = '';
                        sendMessage(message);
                    }
                });
            }
            
            // Evento de pressionar Enter no input
            if (userInput) {
                userInput.addEventListener('keypress', (event) => {
                    if (event.key === 'Enter') {
                        const message = userInput.value.trim();
                        if (message) {
                            addMessage(message, true);
                            userInput.value = '';
                            sendMessage(message);
                        }
                    }
                });
            }
            
            // Evento de clique nas sugestões
            suggestionChips.forEach(chip => {
                chip.addEventListener('click', () => {
                    const message = chip.textContent;
                    addMessage(message, true);
                    sendMessage(message);
                });
            });
            
            // Mobile menu toggle com overlay e animação do ícone
            const mobileMenuBtn = document.querySelector('.mobile-menu-btn');
            const navMenu = document.querySelector('.nav-menu');
            const menuOverlay = document.querySelector('.menu-overlay');
            
            if (mobileMenuBtn) {
                mobileMenuBtn.addEventListener('click', () => {
                    navMenu.classList.toggle('active');
                    mobileMenuBtn.classList.toggle('active');
                    menuOverlay.classList.toggle('active');
                    
                    // Alternar ícone entre barras e X
                    const icon = mobileMenuBtn.querySelector('i');
                    if (navMenu.classList.contains('active')) {
                        icon.className = 'fas fa-times';
                    } else {
                        icon.className = 'fas fa-bars';
                    }
                    
                    // Prevenir scroll do corpo quando menu está aberto
                    document.body.style.overflow = navMenu.classList.contains('active') ? 'hidden' : '';
                });
            }
            
            // Fechar menu ao clicar no overlay
            if (menuOverlay) {
                menuOverlay.addEventListener('click', () => {
                    navMenu.classList.remove('active');
                    mobileMenuBtn.classList.remove('active');
                    menuOverlay.classList.remove('active');
                    mobileMenuBtn.querySelector('i').className = 'fas fa-bars';
                    document.body.style.overflow = '';
                });
            }
            
            // Fechar menu ao clicar em um item no mobile
            const navLinks = document.querySelectorAll('.nav-menu a');
            navLinks.forEach(link => {
                link.addEventListener('click', () => {
                    if (window.innerWidth <= 992) {
                        navMenu.classList.remove('active');
                        mobileMenuBtn.classList.remove('active');
                        menuOverlay.classList.remove('active');
                        mobileMenuBtn.querySelector('i').className = 'fas fa-bars';
                        document.body.style.overflow = '';
                    }
                });
            });
            
            // Modal de login (será implementado posteriormente)
            const loginBtn = document.querySelector('.login-btn');
            if (loginBtn) {
                loginBtn.addEventListener('click', () => {
                    alert('Funcionalidade de login será implementada em breve!');
                });
            }
            
            // Botão de cadastro (será implementado posteriormente)
            const signupBtn = document.querySelector('.signup-btn');
            if (signupBtn) {
                signupBtn.addEventListener('click', () => {
                    alert('Funcionalidade de cadastro será implementada em breve!');
                });
            }
            
            // Controlador para modal de informações dos ícones
            const iconInfo = document.querySelector('.icon-info');
            const closeInfo = document.querySelector('.close-info');
            
            if (closeInfo) {
                closeInfo.addEventListener('click', () => {
                    iconInfo.style.display = 'none';
                });
            }
            
            // Função para mostrar informações quando um ícone é clicado
            window.showIconInfo = function(module) {
                const infoContent = document.querySelector('.info-content');
                
                // Conteúdo dinâmico baseado no módulo clicado
                let content = '';
                switch(module) {
                    case 'oficina':
                        content = '<h2>Oficina</h2><p>Gerenciamento de manutenção de equipamentos e veículos.</p>';
                        break;
                    case 'confinamento':
                        content = '<h2>Confinamento</h2><p>Controle de gado e manejo animal.</p>';
                        break;
                    case 'armazem':
                        content = '<h2>Armazém</h2><p>Gestão de estoque de produtos agrícolas.</p>';
                        break;
                    case 'estoque':
                        content = '<h2>Estoque</h2><p>Controle de insumos e materiais.</p>';
                        break;
                    case 'combustivel':
                        content = '<h2>Posto de Combustível</h2><p>Gerenciamento de abastecimento e consumo.</p>';
                        break;
                    default:
                        content = '<h2>Módulo</h2><p>Informações sobre este módulo estarão disponíveis em breve.</p>';
                }
                
                infoContent.innerHTML = content;
                iconInfo.style.display = 'flex';
            };
            
            // Adicionar listener para fechar modal ao clicar fora do conteúdo
            iconInfo.addEventListener('click', (e) => {
                if (e.target === iconInfo) {
                    iconInfo.style.display = 'none';
                }
            });
            
            // Função para mostrar informações de combustível
            window.showFuelInfo = function() {
                const fuelInfoPopup = document.getElementById('fuel-info-popup');
                fuelInfoPopup.classList.add('active');
            };
            
            // Função para mostrar informações do armazém de grãos
            window.showGrainStorageInfo = function() {
                const grainStoragePopup = document.getElementById('grain-storage-popup');
                grainStoragePopup.classList.add('active');
            };
            
            // Configurar fechamento do popup de combustível
            const fuelInfoPopup = document.getElementById('fuel-info-popup');
            const fuelInfoClose = document.getElementById('fuel-info-close');
            
            fuelInfoClose.addEventListener('click', () => {
                fuelInfoPopup.classList.remove('active');
            });
            
            fuelInfoPopup.addEventListener('click', (e) => {
                if (e.target === fuelInfoPopup) {
                    fuelInfoPopup.classList.remove('active');
                }
            });
            
            // Configurar fechamento do popup do armazém de grãos
            const grainStoragePopup = document.getElementById('grain-storage-popup');
            const grainStorageClose = document.getElementById('grain-storage-close');
            
            grainStorageClose.addEventListener('click', () => {
                grainStoragePopup.classList.remove('active');
            });
            
            grainStoragePopup.addEventListener('click', (e) => {
                if (e.target === grainStoragePopup) {
                    grainStoragePopup.classList.remove('active');
                }
            });
            
            // Configurar as abas do popup de combustível
            const fuelTabs = document.querySelectorAll('.fuel-tab');
            fuelTabs.forEach(tab => {
                tab.addEventListener('click', () => {
                    // Remover a classe active de todas as abas
                    fuelTabs.forEach(t => t.classList.remove('active'));
                    // Adicionar a classe active na aba clicada
                    tab.classList.add('active');
                    
                    // Esconder todos os conteúdos
                    document.querySelectorAll('.fuel-consumption, .fuel-maintenance, .fuel-history').forEach(content => {
                        content.classList.remove('active');
                    });
                    
                    // Mostrar o conteúdo correspondente à aba clicada
                    const tabName = tab.getAttribute('data-tab');
                    document.querySelector(`.fuel-${tabName}`).classList.add('active');
                });
            });
            
            // Configurar as abas do popup do armazém de grãos
            const grainTabs = document.querySelectorAll('.grain-tab');
            grainTabs.forEach(tab => {
                tab.addEventListener('click', () => {
                    // Remover a classe active de todas as abas
                    grainTabs.forEach(t => t.classList.remove('active'));
                    // Adicionar a classe active na aba clicada
                    tab.classList.add('active');
                    
                    // Esconder todos os conteúdos
                    document.querySelectorAll('.grain-inventory, .grain-movement, .grain-reports').forEach(content => {
                        content.classList.remove('active');
                    });
                    
                    // Mostrar o conteúdo correspondente à aba clicada
                    const tabName = tab.getAttribute('data-tab');
                    document.querySelector(`.grain-${tabName}`).classList.add('active');
                });
            });
        });
    </script>
</body>
</html>
